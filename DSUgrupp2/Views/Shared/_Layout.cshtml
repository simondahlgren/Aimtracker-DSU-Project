<!DOCTYPE html>
<html lang="en">
<head>
    <meta charowset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DSUgrupp2</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" /> -->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" /> 
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" integrity="sha384-DyZ88mC6Up2uqS4h/KRgHuoeGwBcD4Ng9SiP4dIRy0EXTlnuz47vAwmeGwVChigm" crossorigin="anonymous">
<script src="https://kit.fontawesome.com/b5817a8ba7.js" crossorigin="anonymous"></script>
<script src="~/js/site.js"></script>
</head>
<body>
    <header>

        <nav class="navbar navbar-expand-sm box-shadow mb-3">
            <div class="container-fluid">
               
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img class="logo" src="https://i.imgur.com/dfS6PBF.png" alt="imageforheader"></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
         
                
                        @{
                            if (User.Identity.IsAuthenticated)
                            {
                               
                                <div class="nav-item1" >
                                    <p class="email">@User.Identity.Name </p>
                                    
                                    <a class="logout" asp-area="" asp-controller="Account" asp-action="Logout">Logout</a>

                                </div>
                            }
                        }

     
                        
            </div>
        </nav>

    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="footer text-muted">
        <div class="container">
            &copy; 2022 - DSUgrupp2
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
   <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
